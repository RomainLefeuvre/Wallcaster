{
  "maxStoreSize": 20,
  "rotationInterval": 0.1,
  "query": {
    "useTwitterApi": false,
    "twitter": {
      "fetchInterval": 5,
      "fetchQuantity": 10,
      "dateRange": {
        "start": "2023-03-16T13:00:00.000Z",
        "end": "2023-03-21T13:00:00.000Z"
      },
      "useWhitelistHashtags": false,
      "whitelistHashtags": [
        "vannes",
        "rennes"
      ],
      "languages" :[
        "fr", // -> French
        "en", // -> English
        "de", // -> German
        "ar", // -> Arabic
        "es", // -> Spanish
        "pl", // -> Polish
        "it", // -> Italian
        "pt", // -> Portuguese
        "ru" // -> Russian
      ],
      "useWhitelistAuthors": false,
      "whitelistAuthors": []
    },
    "useRandomApi": true,
    "random": {
      "fetchInterval": 2
    }
  },
  "filter": {
    "useSentiment": false,
    "useBanwords": false,
    "useBlockImages": false,
    "forbiddenWords": []
  }
}